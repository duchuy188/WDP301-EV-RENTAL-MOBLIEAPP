{
    "expo": {
        "name": "EV Renter",
        "slug": "EV Renter",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "infoPlist": {
                "NSCameraUsageDescription": "Ứng dụng cần truy cập camera để chụp ảnh đại diện và tài liệu xác thực.",
                "NSPhotoLibraryUsageDescription": "Ứng dụng cần truy cập thư viện ảnh để chọn ảnh đại diện."
            }
        },
        "android": {
            "permissions": [
                "CAMERA",
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE"
            ]
        },
        "web": {
            "bundler": "metro",
            "output": "single",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-font",
            "expo-web-browser", [
                "expo-image-picker",
                {
                    "photosPermission": "Ứng dụng cần truy cập thư viện ảnh để chọn ảnh đại diện.",
                    "cameraPermission": "Ứng dụng cần truy cập camera để chụp ảnh đại diện và tài liệu xác thực."
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}